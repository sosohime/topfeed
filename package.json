{
  "name": "@topfeed/topfeed",
  "description": "topfeed",
  "version": "0.0.7",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "scripts",
    "dist",
    "middleware.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TopFeed/topfeed.git"
  },
  "keywords": [
    "react",
    "koa",
    "ssr"
  ],
  "author": "hardfist",
  "bugs": {
    "url": "https://github.com/TopFeed/topfeed/issues"
  },
  "homepage": "https://github.com/TopFeed/topfeed#readme",
  "scripts": {
    "clean": "rimraf dist && rimraf lib",
    "build": "npm run clean && npm run build:typings && tsc",
    "build:typings": "tsc && node ./typing",
    "build:all": "bash ./scripts/build.sh",
    "tsc": "tsc ",
    "dev": "tsc --watch",
    "postinstall": "node ./scripts/post_install.js",
    "test": "jest",
    "lint": "eslint . --cache --fix",
    "size": "bundlesize",
    "pretest": "npm run lint && tsc --noEmit",
    "precommit": "lint-staged",
    "format": "prettier --write '**/*.{js,css,md}'",
    "posttest": "npm run format",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/koa": "^2.0.46",
    "bundlesize": "^0.17.0",
    "dts-bundle": "^0.7.3",
    "dts-bundle-generator": "^1.6.1",
    "eslint": "^5.2.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "prettier": "^1.14.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.62.0",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-typescript2": "^0.15.0",
    "rollup-plugin-uglify": "^3.0.0",
    "typescript": "^3.0.1",
    "uglify-es": "^3.3.9"
  },
  "license": "MIT",
  "lint-staged": {
    "*.{js,ts,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "koa": "^2.5.2",
    "rndm": "^1.2.0"
  }
}
